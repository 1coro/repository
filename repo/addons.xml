<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.befun.cz" name="befun.cz" provider-name="Libor Zoubek" version="1.0.19">
  <requires>
    <import addon="xbmc.python" version="2.1.0" />
    <import addon="script.module.stream.resolver" version="1.6.23" />
  </requires>
  <extension library="default.py" point="xbmc.python.pluginsource" provides="video">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">befun.cz</summary>
    <summary lang="cs">befun.cz</summary>
    <description lang="en">Watch online HD movies and series from befun.cz</description>
    <description lang="cs">Sledujte online HD filmy seriály se servru befun.cz</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="script.module.stream.resolver" name="Stream Resolver" provider-name="Libor Zoubek" version="1.6.24">
  <requires>
    <import addon="xbmc.python" version="2.1.0" />
    <import addon="script.module.simplejson" version="2.0.10" />
  </requires>
  <extension library="lib" point="xbmc.python.module" />
  <extension point="xbmc.addon.metadata">
    <summary lang="cs">Kodi CZ/SK plugins common python modules mainly for resolving stream URLs</summary>
    <description lang="cs">Common python modules pack that is used by kodi cz/sk plugins</description>
    <license>GNU GPL</license>
    <platform>all</platform>
  </extension>
</addon>

